{
  "name": "@khalidsaidi/topoloom",
  "version": "0.2.7",
  "description": "Topology-first graph drawing kernel: embeddings, dual routing, decompositions, and coordinate pipelines.",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./graph": {
      "types": "./dist/graph/index.d.ts",
      "import": "./dist/graph/index.js"
    },
    "./dfs": {
      "types": "./dist/dfs/index.d.ts",
      "import": "./dist/dfs/index.js"
    },
    "./planarity": {
      "types": "./dist/planarity/index.d.ts",
      "import": "./dist/planarity/index.js"
    },
    "./embedding": {
      "types": "./dist/embedding/index.d.ts",
      "import": "./dist/embedding/index.js"
    },
    "./dual": {
      "types": "./dist/dual/index.d.ts",
      "import": "./dist/dual/index.js"
    },
    "./decomp": {
      "types": "./dist/decomp/index.d.ts",
      "import": "./dist/decomp/index.js"
    },
    "./order": {
      "types": "./dist/order/index.d.ts",
      "import": "./dist/order/index.js"
    },
    "./flow": {
      "types": "./dist/flow/index.d.ts",
      "import": "./dist/flow/index.js"
    },
    "./layout": {
      "types": "./dist/layout/index.d.ts",
      "import": "./dist/layout/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "test": "vitest run --coverage",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "devDependencies": {
    "fast-check": "^4.5.3",
    "tsup": "^8.5.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://topoloom.web.app/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khalidsaidi/topoloom.git"
  },
  "bugs": {
    "url": "https://github.com/khalidsaidi/topoloom/issues"
  },
  "keywords": [
    "graph",
    "graph-layout",
    "graph-drawing",
    "graph-visualization",
    "planarity",
    "planar-embedding",
    "rotation-system",
    "half-edge",
    "dual-graph",
    "spqr",
    "biconnected",
    "st-numbering",
    "orthogonal-drawing",
    "min-cost-flow",
    "planarization"
  ]
}
